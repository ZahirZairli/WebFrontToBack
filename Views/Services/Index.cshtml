@model List<Service>
@{
    ViewData["Title"] = "Services";
}
<!-- Start Service -->
<section class="container overflow-hidden py-5">
    <div id="mainDiv" class="row gx-5 gx-sm-3 gx-lg-5 gy-lg-5 gy-3 pb-3">
        <!-- Start Recent Work -->
        @*@Html.Partial("_ServicesPartialView");*@
        @await Component.InvokeAsync("Service")

        <!-- Start Recent Work -->
    </div>
        <div class="d-flex justify-content-center">
        <a id="btnLoadMore" class="btn btn-outline-primary">Load More</a>
    <div id="loader" style="display:none;" class="spinner-border text-info" role="status">
        <span class="sr-only"></span>
    </div>
        </div>
</section>
<input hidden value="@ViewBag.count" id="element"/>
<!-- End Service -->


